// array of objects reference pt2

const books = [
  {
    title: "The Last Kingdom",
    rating: 8.38,
    author: "Bernard Cornwell",
  },
  {
    author: "Robert Sheckley",
    rating: 8.51,
    title: "Beside Still Waters",
  },
  {
    title: "Sen śmiesznego człowieka",
    author: "Fiodor Dostojewski",
    rating: 7.75,
  },
];

const newBook = {
    author: "Chuck Palahniuk",
    rating: 6.05,
    title: "Choke",
}


books.push(newBook)

function changeAllRatings(arrayOfBooks){
    arrayOfBooks.forEach(book => {
        book.rating += 1;
    })
}

console.log(books)

changeAllRatings(books)

console.log(books)