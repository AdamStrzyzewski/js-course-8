// function multiply simple with defaults
// parameters x,y,z
function multiply(x = 1, y = 1, z = 1) {
    console.log(`x * y * z = ${x} * ${y} * ${z} = ${x*y*z}`)
}

console.log("before function call")
// arguments 5,2 (default z = 1)
multiply()
multiply(5)
multiply(5,2)
multiply(5,2,6)

console.log("after function call")

