// function withdraw money
// parameters x,y,z
function withdraw(amount, balance){
    if(amount === 0){
        console.log("you cannot withdraw nothing")
        return;
    }
    
    if(amount > balance){
        console.log("you are just a little bit poor");
        return;
    }
    
    console.log("you have withdrew your money")
    return;
}

withdraw(100, 2000)
withdraw(100, 50)
withdraw(0, 20)